        // a 곱하기 b = ###
        // a 나누기 b = ##
        // a 나누기 b의 나머지 : ###
import java.util.Scanner;
public class Hello {
	int i = 0;

	while(i < 3) {
		System.out.println("haram");
		i++;
		
		
		
		

	}

}
