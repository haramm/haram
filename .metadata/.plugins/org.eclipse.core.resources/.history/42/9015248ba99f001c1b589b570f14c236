        // 매소드 사용 X , a , b => 상수를 직접 입력!!!!


        //출력예시 정수 --> 실수!!!
        //a + b = ###
        // a - b = ###
        // a 곱하기 b = ###
        // a 나누기 b = ##
        // a 나누기 b의 나머지 : ###
import java.util.Scanner;
public class Hello {

	public static void main(String[] args) {
		int i = 0;
		while(i<3) {
			System.out.println("haram");
			i++;
		}
		
		
		
		
		

	}

}
